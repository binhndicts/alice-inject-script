{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2015.promise",
      "es2015",
      "es2016",
      "es5",
      "esnext"
    ],
    "declaration": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "removeComments": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "importHelpers": true,
    "noImplicitReturns": true,
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "rootDir": "./",
    "outDir": "./build"
  }
}
